// Task 2: Exploring data types
let a = 10                 
let b = "10"              
let c = true              
let d = null             
let e = undefined    
let f = { name: "Varsitha" }

console.log(typeof a, typeof b, typeof c, typeof d, typeof e, typeof f);


let strNum = "123"
let numStr = 456

console.log("String to Number:", Number(strNum))
console.log("Number to String:", String(numStr))


console.log("10" + 5)
console.log("10" - 5)
